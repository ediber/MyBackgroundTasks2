<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/start_alarm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="72dp"
        android:text="start_alarm"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/start_forground_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="start_forground_service"
        app:layout_constraintStart_toStartOf="@+id/start_alarm"
        app:layout_constraintTop_toBottomOf="@+id/start_alarm" />

    <Button
        android:id="@+id/start_coroutines"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="start_coroutines"
        app:layout_constraintStart_toStartOf="@+id/start_forground_service"
        app:layout_constraintTop_toBottomOf="@+id/start_forground_service" />

    <Button
        android:id="@+id/start_work_manager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="start_work_manager"
        app:layout_constraintStart_toStartOf="@+id/start_coroutines"
        app:layout_constraintTop_toBottomOf="@+id/start_coroutines" />
</androidx.constraintlayout.widget.ConstraintLayout>